<h1><%= @name %></h1>
<img src="<%= @photo %>" alt="<%= @name %>" style="float:left" width="250" height="300">
<p><%= @description %></p>
<h2><%= @name %>'s courses</h2>
<ul>
<% @courses.each do |element| %>
	  <li><a href="<%= element[:url] %>" class="simple-link"><%= element[:name] %></a></li>
<% end %>
</ul>

<%= render "shared/structural_link" %>
<%= render "shared/semantic_link" %>
