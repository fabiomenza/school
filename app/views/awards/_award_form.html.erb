<%= form_for what, url:{ controller: 'awards', action: to_do}, method: to_met do |f| %>
	<p>
		<%= f.label :title %> 
		<%= f.text_field  :title   %> <br/>
	</p>
	<p> 
		<%= f.label :description %> <br/>
    	<%= f.text_area :description %>
	</p>
	<p>
		
		<%= f.label :date %> <br/>
		<%= f.date_select :date, order: [:day, :month, :year], start_year: 1970 %>
	</p>
	<%= f.submit %>
<% end %>

