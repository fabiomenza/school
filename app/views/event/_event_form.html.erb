<% if defined? time %>
	<%= debug time %>
<% end %>

<%= form_for what, url:{ controller: 'event', action: to_do}, method: to_met do |f| %>
	<%= f.label :name %> <br>
	<%= f.text_field :name %> <br><br>
	<%= f.label :description%> <br>
	<%= f.text_area :description %> <br>
	<%= f.label :get_involved%> <br>
	<%= f.text_area :get_involved %> <br>
	<%= f.label :structure%> <br>
	<%= select_tag(:structure, options_from_collection_for_select(Structure.all, :id, :name)) %> <br>
	<%= f.label :event_type%> <br>
	<%= select_tag(:event_type, options_from_collection_for_select(EventType.all, :id, :name)) %> <br>
	<br>
	<%= f.submit %>
<% end %>
