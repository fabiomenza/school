<%=  %><!DOCTYPE html>
<html>
<head>
  <title><%= @title %></title>
  
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>

        <!-- load jQuery -->
        <script src="http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.js"></script>

        <!-- load Galleria -->
        <script src="/assets/galleria-1.3.5.min.js"></script>

	  <script src="//code.jquery.com/jquery-1.10.2.js"></script>
	  <script src="//code.jquery.com/ui/1.10.4/jquery-ui.js"></script>
	  <link rel="stylesheet" href="/resources/demos/style.css">
	  <script>
	  $(function() {
	    $( "#datepicker" ).datepicker();
	  });
	  </script>
</head>
<body>
<div id=main-wrapper>
  <div id='admin_menu'>
	<%= render "shared/admin_menu" %>
  </div>
  <div id=header-wrapper>
      <a href="/"><%= image_tag "logo.jpg", id: "logo" %> </a>
    </div><!-- end of header-wrapper -->
    <div id=nav>
    	<%= render "shared/landmark_menu" %>
    </div>
    <div id=page-wrapper>
      <div id=content>
      	<% flash.each do |name, msg| %>
      		<%= content_tag :div, id: "flash_#{name}" do%>
				<%= msg %>
				

      		<% end %>
      	<% end %>
		<!-- Qui viene reso il contenuto della pagina -->
		<%= yield %>

      </div>
	  <div id=sidebar-right>
	    <div id=structural-block>
	      
			<%= yield :structuralLinkMenu %>
	      
	    </div> 
	    <div id=semantic-block>
	      
	      		<%= yield :semanticLinkMenu %>
	      
	    </div>
	</div>
      </div>


    <div id=footer-wrapper>
		
		<%= render "shared/footer_menu" %>
		<div id=credits>
			
		</div>
    </div>
</div>
</body>
</html>
