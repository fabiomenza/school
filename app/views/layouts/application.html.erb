<%=  %><!DOCTYPE html>
<html>
<head>
  <title><%= @title %></title>
  
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>

        <!-- load jQuery -->
        <script src="http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.js"></script>

        <!-- load Galleria -->
        <script src="/assets/galleria-1.3.5.min.js"></script>
</head>
<body>
<div id=main-wrapper>
  <div id=header-wrapper>
      <a href="/"><%= image_tag "logo.jpg", id: "logo" %> </a>
    </div><!-- end of header-wrapper -->
    <div id=nav>
    	<%= render "shared/landmark_menu" %>
    </div>
    <div id=page-wrapper>
      <div id=content>
		<!-- Qui viene reso il contenuto della pagina -->
		<%= yield %>

      </div>
	  <div id=sidebar-right>
	    <div id=structural-block>
	      <ul class=menu>
			<%= yield :structuralLinkMenu %>
	      </ul>
	    </div> 
	    <div id=semantic-block>
	      <ul class=menu>
	      		<%= yield :semanticLinkMenu %>
	      </ul>
	    </div>
	</div>
      </div>


    <div id=footer-wrapper>
		
		<%= render "shared/footer_menu" %>
		<div id=credits>
			<strong> bannana</strong>
		</div>
    </div>
</div>
</body>
</html>
